        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:drawable="@color/colorOrange"/>
    <item android:top="4dp"
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:drawable="@color/colorWhite"/>
